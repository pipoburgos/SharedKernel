<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;net481;netstandard2.0;netstandard2.1;net8.0;net9.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsPackable>true</IsPackable>
    <Authors>Roberto Fernández Salinas</Authors>
    <Company>Roberto Fernández Salinas</Company>
    <Description>PayPal</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>latest</LangVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageIcon>icon.png</PackageIcon>
    <PackageId>Pipoburgos.PayPal</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    <PackageTags>PayPal</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/pipoburgos/SharedKernel</RepositoryUrl>
    <Version>9.0.5.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="V1\Payments\Authorizations\**" />
    <Compile Remove="V1\Payments\BillingAgreements\**" />
    <Compile Remove="V1\Payments\Captures\**" />
    <Compile Remove="V1\Payments\Orders\**" />
    <Compile Remove="V1\Payments\Payouts\**" />
    <Compile Remove="V1\Payments\Refunds\**" />
    <Compile Remove="V1\Payments\Sales\**" />
    <Compile Remove="V1\Shared\ModelsToMigrate\**" />
    <EmbeddedResource Remove="V1\Payments\Authorizations\**" />
    <EmbeddedResource Remove="V1\Payments\BillingAgreements\**" />
    <EmbeddedResource Remove="V1\Payments\Captures\**" />
    <EmbeddedResource Remove="V1\Payments\Orders\**" />
    <EmbeddedResource Remove="V1\Payments\Payouts\**" />
    <EmbeddedResource Remove="V1\Payments\Refunds\**" />
    <EmbeddedResource Remove="V1\Payments\Sales\**" />
    <EmbeddedResource Remove="V1\Shared\ModelsToMigrate\**" />
    <None Remove="V1\Payments\Authorizations\**" />
    <None Remove="V1\Payments\BillingAgreements\**" />
    <None Remove="V1\Payments\Captures\**" />
    <None Remove="V1\Payments\Orders\**" />
    <None Remove="V1\Payments\Payouts\**" />
    <None Remove="V1\Payments\Refunds\**" />
    <None Remove="V1\Payments\Sales\**" />
    <None Remove="V1\Shared\ModelsToMigrate\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="" />
    <None Include="readme.md" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
